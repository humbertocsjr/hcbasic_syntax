{
  "name": "hcbasic",
  "displayName": "hcbasic",
  "description": "Highlight Syntax support for HCBasic language",
  "version": "0.0.1",
  "repository": {"url": "https://github.com/humbertocsjr/hcbasic_syntax"},
  "publisher": "Humbertocsjr",
  "engines": {
    "vscode": "^1.72.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hcbasic",
        "aliases": [
          "HCBasic",
          "t3x"
        ],
        "extensions": [
          ".hcb"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hcbasic",
        "scopeName": "source.hcbasic",
        "path": "./syntaxes/hcbasic.tmLanguage.json"
      }
    ]
  },
  "dependencies": {
    "nvm": "0.0.4"
  }
}
